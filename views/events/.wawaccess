wawaccess do
  
  inherits true
  
  match /register\/(.*)$/ do |url, event|
    tuple = AcmScW::database[:webr_planned_events].filter(:id => event).first
    people = {}
    if tuple.nil?
      apply "/404"
    else
      wlang "register.form", wlang_context.merge(:event => tuple, :people => people)
    end
  end
  
end