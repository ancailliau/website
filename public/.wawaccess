wawaccess do
  strategy :allow_all

  # We serve all as static file by default
  serve file do |url, realpath, wawaccess, env|
    #puts "Serving file for #{url}"
    wawaccess.static url, env
  end

  # When something is not found, we look in the pages wawaccess
  notfound do |url, realpath, wawaccess, env|
    #puts "Delegating to pages #{url}"
    wawaccess.apply "/pages/#{url}", env
  end

end